<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>IdenticonServlet</servlet-name>
        <servlet-class>com.docuverse.identicon.IdenticonServlet</servlet-class>
        <init-param>
            <param-name>inetSalt</param-name>
            <param-value>thisisaincreadiblyinsanesalt</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>identiconServlet</servlet-name>
        <servlet-class>identiconServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyIdenticonServlet</servlet-name>
        <servlet-class>Heng.MyIdenticonServlet</servlet-class>
        <init-param>
            <param-name>inetSalt</param-name>
            <param-value>thisisaincreadiblyinsanesalt</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>IdenticonServlet</servlet-name>
        <url-pattern>/IdenticonServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>identiconServlet</servlet-name>
        <url-pattern>/identiconServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyIdenticonServlet</servlet-name>
        <url-pattern>/MyIdenticonServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
